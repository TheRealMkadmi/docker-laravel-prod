#!/command/execlineb -P
fdmove -c 2 1
with-contenv
cd /var/www/html
pipeline { php artisan octane:start --server=swoole --host=0.0.0.0 --port=8000 --workers=${OCTANE_WORKERS} --task-workers=${OCTANE_TASK_WORKERS} --max-requests=${OCTANE_MAX_REQUESTS} --max-execution-time=30 --backlog=${OCTANE_BACKLOG} }
/usr/local/bin/logger.sh OCTANE INFO

